<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<!DOCTYPE html>
<html>
<head>
    <title>我的</title>
    <%@include file="/common/headModule.jsp" %>
    <link rel="stylesheet" type="text/css" href="${path}/resources/css/public1.css"/>
    <link rel="stylesheet" type="text/css" href="${path}/resources/css/my.css"/>
    <script type="text/javascript" src="${path}/resources/js/index.js"></script>
</head>

<body style="background:#f5f5f5;">
<div class="personal_top">
    <button class="top-right" onClick="window.location='${path}/admin/wechat/setting/index.html?shopId=${shopId}';"><img class="top-right" src="${path}/resources/images/top_btn_shezhi.png"></button>
    <%--客服按钮--%>
    <%--<button class="top-right02" onClick=""><img class="top-right" src="${path}/resources/images/top_btn_xiaoxi.png"></button>--%>
    <div class="personal_index">
        <table class="personal_jieshao" style="padding-left:5%; padding-top:4px; height:240px;">
            <tbody>
            <tr>
                <th>
                    <img id="userPhoto" class="personal_touxiang" style="border-radius:40px;" onClick="window.location='${path}/admin/wechat/personal/index.html?shopId=${shopId}';">
                    <h3 id="userName"></h3>
                </th>
            </tr>
            </tbody>
        </table>
        <div class="dingdan">
            <table class="indexTabs">
                <tr>
                    <td width="14%" onClick="window.location='${path}/admin/wechat/cusOrder/index.html?shopId=${shopId}';">
                        <img class="myIcon" src="${path}/resources/images/my_ic_dingdan.png">
                        <h2>订单</h2>
                    </td>
                    <td width="14%" onClick="window.location='${path}/admin/wechat/cusCart/index.html?shopId=${shopId}'">
                        <img class="myIcon" src="${path}/resources/images/my_ic_gouwuche.png">
                        <h2>购物车</h2>
                    </td>
                    <td width="14%" onClick="window.location='${path}/admin/wechat/coupon/index.html?shopId=${shopId}';">
                        <img class="myIcon" src="${path}/resources/images/my_ic_daijinquan.png">
                        <h2>代金券</h2>
                    </td>
                    <%--<td width="14%" onClick="window.location='2-4-7wodepintuan.html';">--%>
                        <%--<img class="myIcon" src="${path}/resources/images/my_ic_pintuan.png">--%>
                        <%--<h2>拼团</h2>--%>
                    <%--</td>--%>
                    <td width="14%" onClick="window.location='${path}/admin/wechat/returnInfo/index.html?shopId=${shopId}';">
                        <img class="myIcon" src="${path}/resources/images/my_ic_tuihuo.png">
                        <h2>退货</h2>
                    </td>
                </tr>
            </table>
        </div>
    </div>

    <div class="dianpuguanli" style="overflow-x: hidden;">
        <p class="biaoti"><img class="dianpuIc" src="${path}/resources/images/my_ic_pindao.png"><font>商品频道</font></p>
        <table class="indexTabs1">
            <tr>
                <td width="33%" onClick="window.location='${path}/admin/wechat/product/index.html?shopId=${shopId}';">
                    <img src="${path}/resources/images/my_ic_jingxuan.png">
                    <h2>精选商品</h2>
                </td>
                <td width="33%" onClick="window.location='${path}/admin/wechat/activity/index.html?shopId=${shopId}';">
                    <img src="${path}/resources/images/my_ic_temai.png">
                    <h2>特卖活动</h2>
                </td>
                <td width="33%" onClick="window.location='${path}/admin/wechat/shop/index.html?shopId=${shopId}';">
                    <img src="${path}/resources/images/my_ic_guanzhu.png">
                    <h2>店主推荐</h2>
                </td>
            </tr>
        </table>
    </div>
</div>
</body>
</html>
