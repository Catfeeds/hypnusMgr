    <title>东明石化商城移动端接口</title>
    <script type="text/javascript">
        var rasPostFun;
        var _attachment;

        seajs.use(['$', 'msgBox', 'validate', 'base64', 'util', 'rsa', 'attachment', 'cookie', 'jquery.json'], function ($, msgBox, validate, base64, util, rsa, attachment) {
            _attachment = attachment;
            var DataHandler = function () {
                return {
                    /**
                     * 获取RAS加密数据
                     * @param param
                     * @param callback
                     */
                    getRasKey: function (callback) {
                        $.post(path + '/pub/common/getRasKey', null, function (backData) {
                            callback(backData);
                        });
                    }
                }
            }();

            var EventHandler = function () {
                return {
                    /**
                     * 获取RAS加密数据
                     * @param obj 点击事件对象
                     * @param url 请求连接
                     */
                    rasPost: function (obj, url) {
                        $('#show').html('');
                        /**
                         * 加密传输密码数据
                         */
                        DataHandler.getRasKey(function (backData) {
                            var modulus = backData.result.modulus;
                            var exponent = backData.result.exponent;
                            var rsaKey = new rsa.RSAKey();
                            rsaKey.setPublic(base64.b64tohex(modulus), base64.b64tohex(exponent));
                            var param = {};
                            var inputs = $(obj).parent("td").children("input");
                            $.each(inputs, function (i, item) {
                                if ($(item).attr("isRas") == 1) { // 需要进行Ras加密
                                    param[$(item).attr("name")] = base64.hex2b64(rsaKey.encrypt($(item).val()));
                                } else {
                                    param[$(item).attr("name")] = $(item).val();
                                }
                            });
                            $.post(path + url, param, function (backData) {
                                msgBox.tips(backData.type);
                                $('#show').html(JSON.stringify(backData));
                            });
                        });
                    }
                }
            }();

            rasPostFun = EventHandler.rasPost;
        });

        /**
         * GET方式调用
         * @param url
         */
        function commonGet(url) {
            $('#show').html('');
            $.get(path + url, null, function (backData) {
                $('#show').html(JSON.stringify(backData));
            });
        }
        /**
         * GET方式（带参数）调用
         * @param url
         */
        function commonGetWithParam(obj, url) {
            $('#show').html('');
            var param = {};
            var inputs = $(obj).parent("td").children("input");
            $.each(inputs, function (i, item) {
                param[$(item).attr("name")] = $(item).val();
            });
            $.get(path + url, param, function (backData) {
                $('#show').html(JSON.stringify(backData));
            });
        }
        /**
         * 上传
         */
        function upload(obj, inputId) {
            var param = {
                //scene: "T_HOMEPAGE_BANNER", //场景化标识
                isDB: 1, // 是否保存到数据库
                limitType: "image" //limitType默认不传就不限制,支持：image/file/media
            };
            _attachment.uploadFile(function (backParam) {
                $(obj).parent("td").children("input[name=" + inputId + "]").val(backParam[0].attachmentId);
            }, param);
        }

    </script>
