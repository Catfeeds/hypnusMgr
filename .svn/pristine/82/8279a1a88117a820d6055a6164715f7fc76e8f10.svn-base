@charset "utf-8";
/* CSS Document */




.tongjibiao{background:#fff;position:relative; }
.tongjibiaoItem{overflow:hidden; background:#fff; border-bottom:1px solid #e5e5e5;}
.tongjibiaoItem1{overflow:hidden; background:#fff; border-bottom:1px solid #e5e5e5; height:76px;}
.tongjibiaoItem h1{float:left; font-size:16px; color:#666; line-height:60px; margin-right:10px; padding-left:20px;}
.tongjibiaoItem h2{float:right; font-size:16px; color:#333; line-height:60px; }
.tongjibiaoItem1 h3{position:absolute; left:20px; top:17px; font-size:16px; color:#666;  }
.tongjibiaoItem1 h4{position:absolute; left:20px; top:37px; font-size:14px; color:#999; }
.tongjiBg01{background:#ea3c34 url(../images/tongji_bg_chart.png) no-repeat center; padding:50px;}
.tongjiBg02{background:#ea3c34 url(../images/tongji_bg_chart02.png) no-repeat center; padding:50px;}
.tongjibiaoItem1{overflow:hidden; background:#fff;}
.tongjibiaoItem1 h1{float:left; font-size:14px; color:#666; line-height:50px; margin-right:10px;}
.tongjibiaoItem1 h2{float:right; font-size:14px; color:#333; line-height:50px; }

.head{float:right; width:40px; margin-top:5px;}
.arrow{ float:right; height:14px; margin:23px 0px 0px 5px; padding-right:20px;}
.arrow1{ float:right; height:14px; margin:31px 0px 0px 5px; padding-right:20px;}
.neirongTop{ background:#fff; padding:30px 0px; }
.neirongTop h2{text-align:center; font-size:16px; font-weight:normal; color:#666; }
.neirongTop h1{text-align:center; font-size:32px; color:#333; padding-top:10px;  }


.neirongBottom{ background:#fff; height:100px; padding:0 160px 0 100px; position:relative;}
.timeBtn{float:left; font-size:14px;height:50px; width:100%; overflow:hidden; position:relative;  width:100%; 
border:1px solid #ccc; border-left:none;  border-radius:0px 5px 5px 0px;}
.neirongBottom p{ position:absolute; left:20px; top:0px;width:80px; line-height:50px; text-align:center; font-size:16px; color:#000; border:1px solid #ccc; border-right:none;  border-radius:5px 0px 0px 5px; }
.neirongBottom a{position:absolute; right:20px; top:0px; width:120px; line-height:50px; text-align:center; font-size:16px; color:#666; border:1px solid #ccc;  border-radius:5px; }
