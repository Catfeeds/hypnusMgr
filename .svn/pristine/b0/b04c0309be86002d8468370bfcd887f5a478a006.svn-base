/*九宫格*/
.personal{  position:relative;}
.indexTabs{ width:96%; border-bottom:1px solid #e5e5e5; background:#fff; position:absolute; top:195px; left:0px; margin:0px 2%; border-radius:5px; box-shadow:0px 2px 1px #adadad;}
.indexTabs tr td{ padding:10px 0; position:relative; text-align:center; cursor:pointer;}
.indexTabs tr td h2{font-size:12px; color:#646464; text-align:center;}
.timeMain{font-size:16px; color:#ffe327; text-align:right; letter-spacing:1px; display:inline-block; float:right;}



