@charset "utf-8";
/* CSS Document */


/*注册页面*/
.login_bg_line{position:fixed; left:0; right:0; top:20px; width:100%;}
.loginLogo{width:100%;}
.loginLogo img{width:160px; height:160px; margin:0 auto; display:block; padding-top:32%;} 
.loginBottom{position:fixed; bottom:30px; left:0; right:0; padding:40px; padding:0 20px; z-index:99;}
.loginLine{background:url(../images/login_img_line.png) no-repeat center; padding-top:40px;}
.bottomText{text-align:center; font-size:15px; color:#f13d2e;}
.login_img_line{width:180px; margin:0 auto;}
.loginBtn button{font-size:18px; border-radius:6px; width:48%; height:50px;}
.zhuceBtn{background:#fff; border:1px solid #f13d2e; color:#f13d2e;float:left;}
.zhuceBtn:hover{opacity:0.8;}
.dengluBtn{background:#f13d2e; border:1px solid #f13d2e; color:#fff; float:right;}
.dengluBtn:hover{background:#e13425;}

/*注册列表填写*/
.loginList{padding:0px 20px; background:#fff;}
.loginTip{font-size:14px; color:#f13d2e;}
.loginItem{overflow:hidden; margin-top:10px;}   
.itemTitle01{float:left; font-size:18px; color:#323232; width:72px; line-height:50px;}
.itemTitle02{float:left; font-size:18px; color:#323232; width:62px; line-height:50px;}
.itemTitle03{float:left; font-size:16px; color:#323232; width:120px; line-height:50px;}
.itemTitle04{float:left; font-size:18px; color:#323232; width:80px; line-height:50px;}
.inputBox{border-bottom:1px solid #ccc; overflow:hidden; position:relative;}
.loginInput{float:left; font-size:15px; color:#646464; width:100%; line-height:50px;}
.scanIcon{width:24px; position:absolute; right:0px; top:12px;}
.nextIcon{width:14px; position:absolute; right:0px; top:18px;}
.nextBtn{background:#f47f30; color:#fff; width:100%; height:50px; border-radius:6px; font-size:18px;}
.nextBtn02{background:#ff4d4d;color:#fff; width:100%; height:50px; border-radius:6px; font-size:18px; }
.nextBtn:hover{background:#e47124; cursor:pointer;}
.nextBtn02:hover{background:#ff7db8;}
.yanzhengma{background:#f13d2e; color:#fff; font-size:14px; position:absolute; top:12px; right:0; border-radius:6px; height:30px; line-height:30px; text-align:center; letter-spacing:1px; width:100px;}
.yanzhengma:hover{background:#e13425; cursor:pointer;}
.selectItem{float:left; margin-right:20px; margin-left:10px; overflow:hidden;}
.imgselect{width:22px; float:left; margin-top:16px;} 
.selectText{font-size:18px; color:#646464; float:left; line-height:50px; margin-left:6px;}



/*企业信息*/
.qiyePic{margin-top:10px;}
.qiyePic table{width:100%; margin-top:10px;}
.qiyePic table tr th{width:48%; position:relative; padding:1%; border:1px solid #e5e5e5;}

.qiyePicImg{width:100%;}
.qiyePicText{font-size:13px; color:#fff; background:#000; opacity:0.6; position:absolute; left:0; bottom:3%; text-align: center; line-height:30px; width:98%; margin:0 1%;}
.qiyePicscText{font-size:13px; color:#fff; background:#ea3c34; position:absolute; left:0; bottom:3%; text-align: center; line-height:30px; width:98%; margin:0 1%;}





