package com.catt.wfx.web.controller.guest;

import com.catt.common.web.controller.BaseController;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

/**
 * Package: com.catt.wrx.web.controller.guest
 * Description:应用级控制器(关于我们，用户协议)
 * date: 2016-01-08 14:29
 * author: zhou mingxiang
 */
@Controller
@RequestMapping("/guest/app")
public class AppController extends BaseController {
    /**
     * 关于我们页面
     *
     * @return
     */
    @RequestMapping(value = "/aboutus.html", method = RequestMethod.GET)
    public String aboutus() {
        return "/app/aboutus";
    }

    /**
     * 用户协议页面
     *
     * @return
     */
    @RequestMapping(value = "/agreement.html", method = RequestMethod.GET)
    public String agreement() {
        return "/app/agreement";
    }

    /**
     * 服务条款页面
     *
     * @return
     */
    @RequestMapping(value = "/termService.html", method = RequestMethod.GET)
    public String termService() {

        return "/app/termService";
    }

    /**
     * 如何成为店主说明页面
     *
     * @return
     */
    @RequestMapping(value = "/shopkeeper.html", method = RequestMethod.GET)
    public String shopkeeper() {

        return "/app/shopkeeper";
    }

    /**
     * 提现说明页面
     *
     * @return
     */
    @RequestMapping(value = "/withdrawalsExplain.html", method = RequestMethod.GET)
    public String withdrawalsExplain() {

        return "/app/withdrawalsExplain";
    }

    /**
     * 购买须知页面
     *
     * @return
     */
    @RequestMapping(value = "/buyTip.html", method = RequestMethod.GET)
    public String buyTip() {

        return "/app/buyTip";
    }

    /**
     * 购买须知页面
     *
     * @return
     */
    @RequestMapping(value = "/profit.html", method = RequestMethod.GET)
    public String profit() {

        return "/app/profit";
    }




}
