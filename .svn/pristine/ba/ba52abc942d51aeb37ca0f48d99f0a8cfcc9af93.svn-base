@charset "utf-8";
/* CSS Document */



.xiangqingTop{ background:#fff; margin:10px 20px 0px 20px; padding-bottom:10px; border-bottom:1px solid #e5e5e5;}
.biaoti{ font-size:16px; line-height:24px; color:#666; }
.neirong{font-size:16px; line-height:24px; color:#333;}
.topNeirong{font-size:16px; line-height:22px; color:#666;}
.dizhi{font-size:16px; line-height:22px; color:#666; }
.kuaidi{ padding:0px 20px; height:70px; border-bottom:10px solid #f5f5f5;}
.kuaidi p{ float:left;font-size:18px; line-height:70px; color:#fa4b89; width:70%;}
.arrow{ float:right; height:14px; margin:28px 0px 0px 5px;}

.goodsList{ margin:10px 0;}
.goodsList h1{ position:relative;}
.goods{ position:relative; padding:0 10px 0px 40px; height:110px; }
.goodsList .goods:last-child{} 
.goodTip{ color:#646464;}
.biaotiRight{ position:absolute; top:16px; right:20px;  font-size:14px; line-height:24px; font-weight:900; }
.biaotiLeft{ position:absolute; top:0; left:140px; height:20px; line-height:20px; font-size:16px; color:#1a1a1a; letter-spacing:1px; padding-right:20px;}
.goods h3{ position:absolute; top:55px; left:140px; font-size:15px; color:#666;}
.jiage{ position:absolute; top:90px; left:140px; font-size:16px; color:#333; font-weight:900;}
.shuliang{ font-weight:normal; color::#808080;}
.goodsMsg{font-size:14px; color:#969696; margin-bottom:6px; }
.goodType{ background:#fff; min-height:100px;border-bottom:1px solid #e5e5e5; }
.buyNum{ position:absolute; top:28px; left:0px; border:1px solid #c4c4c4; width:104px; height:28px;}
.gouwuchePic{  position:absolute; top:0; left:20px;}

.tuikuan{float:right; font-size:14px; color:#666; line-height:35px; text-align:right; padding:0px 25px; border:1px solid #ccc; border-radius:5px; margin-top:10px; margin-right:20px;}

ul.DDPrice{}
ul.DDPrice li{ height:40px; line-height:40px; border-bottom:1px solid #efefef; margin:0px 20px;}
ul.DDPrice li h1{float:left; font-size:16px; color:#888; line-height:40px; margin-right:10px;}
ul.DDPrice li h2{float:right; font-size:16px; color:#333; line-height:40px; position:relative;}
ul.DDPrice li h2 img{ position:absolute; top:12px; right:-6px;}
ul.DDPrice li.both{overflow:hidden;  border-bottom:none;}
ul.DDPrice li.both h1{float:left; font-size:16px; font-weight:900; color:#333; line-height:40px; margin-right:10px;}
ul.DDPrice li.both h2{float:right; font-size:18px; font-weight:900; color:#fa4b89; line-height:40px; }

/*底部浮动*/
.baomingList{background:#fff; margin-top:10px;}
.baodaoTip{position:absolute; right:0; top:0; font-size:12px; border-radius:4px; padding:2px 4px; letter-spacing:1px;}
.zhifuBottom{position:fixed; bottom:0; left:0; right:0; height:50px; overflow:hidden; background:#fff; box-shadow:0 -1px 6px #b7b7b7;}
.baomingNum{float:right; line-height:40px; margin-right:20px;}
.priceText{float:left; font-size:14px; color:#646464; line-height:50px; padding-left:20px; letter-spacing:1px;}
.priceNum{font-size:28px; font-weight:bold;}
.zhifuBtn{float:right; width:120px; font-size:16px; letter-spacing:1px; line-height:50px; background:#ff4d4d; color:#fff;}
.textpriceNum{ float:left;width:120px; margin-top:6px; height:35px; line-height:30px; border:1px solid #efefef;; background:#f9f9f9; font-size:18px; font-weight:bold; color:#f13d2e;}

.zhifu button{ width:90px; height:35px; line-height:30px; text-align:center; display:block; border-radius:5px; float:right; font-size:16px; letter-spacing:1px;}
