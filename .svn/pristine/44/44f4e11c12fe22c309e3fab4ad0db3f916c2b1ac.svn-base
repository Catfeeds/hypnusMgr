body{ background:#e5e5e5;}
/*-------头部-------*/
.head{ height:47px; position:fixed; top:0; left:0; width:100%; z-index:99; background:#fff;border-bottom:1px solid #dcdcdc;}
.top-left{ position:absolute;width:38px;height:38px;top:6px;left:0%;font-size:18px;color:#FFF;}
.top-left img{width:26px; height:26px;}

.top-right{ position:absolute;width:38px;height:38px;top:6px;right:8px;font-size:18px;color:#FFF; }
.top-right02{ position:absolute;width:38px;height:38px;top:6px;right:52px;font-size:18px;color:#FFF;}
.top-right03{ position:absolute;width:38px;height:38px;top:6px;right:94px;font-size:18px;color:#FFF;}
.top-right:hover{opacity:0.7;}
.top-right_send{float:right; position:absolute; width:100px; height:40px; top:4px; right:5px; font-size:18px; color:#fff; text-align:center; line-height:40px; background:#8bcc46;}
.head h1{width:100%; font-size:18px;color:#323232;margin:0px auto; line-height:46px; text-align:center; border-bottom:1px solid #e5e5e5;position:absolute; border-bottom:none; color:#222;}

/*---*/
table{ border-collapse:collapse; width:100%;}
.floatLeft{ float:left;}
.floatRight{ float:right;}
.textRed{ color:#ff4d4d;}
.bgRed{ background:#fa4b89;}
.bgYellow{ background:#ff934b;}
.bgBlue{ background:#4bb2fa;}


/*-------我的店铺-------*/
.storte_top{width:100%;}
.bg_store{ position:relative; background:#fff;}
.wode{ position:absolute; top:20%; left:40%; width:100px;}
.wode img{ margin-left:10px;}
.wode h2{ font-size:18px; color:#fff; height:30px; line-height:30px;  text-align:center; margin-bottom:3px;}
.wode h3{ width:100px; height:26px; line-height:26px; text-align:center; background:#fff; border-radius:10px; letter-spacing:1px; color:#ff4d4d; font-size:14px;}

/*-------单品管理-------*/
.store_main{}
table.goodsType{ width:100%; height:35px; background:#fff; border-bottom:1px solid #e5e5e5; margin-bottom:10px;}
table.goodsType td a{ display:block; margin:0 10%; height:40px; line-height:40px; text-align:center; font-size:16px; color:#222; cursor:pointer; border-bottom:2px solid #fff;}
table.goodsType td a.lion{ border-bottom:2px solid #ff4d4d; color:#ff4d4d;}
.goodsList{ border-top:1px solid #e5e5e5;}
.goodsDiv{ border-bottom:1px solid #e5e5e5; padding:10px 0; background:#fff;}
.goodsHead{ padding:10px 10px 10px 120px; position:relative; height:100px;}
.goodsHead img{ position:absolute; top:10px; left:10px;}
.goodsHead h1{ height:44px; line-height:22px; font-size:16px; color:#222; margin-bottom:3px;}
.goodsHead h2{ color:#555; font-size:14px; height:30px; line-height:26px;}
.goodsHead h2 span{ font-size:15px; color:#ff4d4d; font-size:22px; padding-right:25px; float:left;}
.goodsHead h2 font{ color:#ff4d4d; font-size:14px;}
.goodsHead h3{ color:#a6a6a6; font-size:12px;}
.goodsHead h3 span{ background:#ff4d4d; width:100px; height:20px; text-align:center; color:#fff; font-size:12px; border-radius:20px; margin-right:12px; padding:0 10px;}

table.goodsBt{ width:100%;}
table.goodsBt td{ height:40px; line-height:40px; text-align:center; font-size:14px; color:#808080; cursor:pointer;}
table.goodsBt td span{ display:block; padding-left:25px; width:30px; margin:0 auto; position:relative;}
table.goodsBt td span img{ position:absolute; top:9px; left:0;}



/*-------拼团-------*/
.ptDiv{ background:#fff; padding-bottom:8px;}
.ptDiv img.banner{ width:100%;}
.ptDiv h1{ font-size:16px; color:#333; height:30px; line-height:30px; margin:0 10px 5px 10px; overflow:hidden;}
.ptDiv h1 font{ color:#ff4d4d; padding-right:8px;}
.ptDiv h2{ font-size:12px; color:#666; height:30px; line-height:30px; margin:0 10px;}
.ptDiv h2 font{ color:#ff4d4d; padding-right:8px; font-size:18px;}
.ptDiv h2 i{ font-style:normal; font-size:12px; display:block; color:#ff4d4d; width:60px; height:18px; line-height:18px; text-align:center; border-radius:8px; border:1px solid #fa509d; float:left; margin-top:5px; margin-right:5px;}
span.ptState{ float:right; background:#ff4d4d; font-size:13px; height:28px; line-height:28px; padding:0 15px; color:#fff; border-radius:8px; text-align:center;}
.ptDiv h3{ color:#a6a6a6; font-size:12px; letter-spacing:1px; height:30px; line-height:30px; margin:0 10px; clear:both;}
.ptDiv h3 font{ color:#ff4d4d;}
span.kucun{ float:left; background:#b2b2b2; font-size:13px; height:20px; line-height:20px; width:70px; color:#fff; border-radius:10px; text-align:center; margin-top:5px;}

/*-------拼团详情-------*/
.pintuanDetail{ margin-top:10px;}
.pintuanStep{ position:relative; background:#fff; margin-bottom:10px; height:130px;}
.pintuanStep h1{ height:50px; line-height:50px; color:#333; font-size:16px; margin:0 15px;}
span.lineBar{ display:block; width:80%; margin:0 10%; height:10px; background:#ffebf4; position:absolute; top:57px; left:0; border-radius:20px;}
.step{ width:60px; min-height:20px; line-height:120%; color:#333; font-size:14px; padding-top:30px; text-align:center; position:absolute;}
span.point{ width:20px; height:20px; line-height:20px; border-radius:20px; background:#ffebf4; border:1px solid #ffa7cf; color:#ffa7cf; font-size:14px; text-align:center; position:absolute; top:5px; left:20px;}
.step01{ top:46px; left:3%;}
.step02{ top:46px; left:30%;}
.step03{ top:46px; right:30%;}
.step04{ top:46px; right:3%;}
.pointUp{ width:20px; height:20px; line-height:20px; border-radius:20px; border:1px solid #ffa7cf; font-size:14px; text-align:center; background:#fa5da3; color:#fff; position:absolute; top:5px; left:20px;}

.imgDiv{}
.imgDiv img{ width:100%;}
.nextBtn1{background:#ff4d4d ;color:#fff; width:100%; height:50px; font-size:18px; position:fixed; bottom:0px; z-index:100; overflow:hidden; font-family:"微软雅黑", Arial; letter-spacing:2px;}


/*-----立即开团-----*/
.edit{ padding:15px 3%;}
.edit h1.ptText{ text-align:right; color:#888; font-size:14px; height:40px; line-height:40px;}
textarea.text{ padding:10px 4%; width:92%; color:#666; font-size:14px; min-height:100px; letter-spacing:1px; border:none;}

.pintuanDiv{ padding:10px 10px 10px 120px; position:relative; height:100px; margin-bottom:10px; background:#fff; cursor:pointer; border-radius:10px; border:1px solid #efefef;}
.pintuanDiv img{ position:absolute; top:10px; left:10px;}
.pintuanDiv h1{ height:30px; line-height:30px; font-size:18px; color:#222; margin-bottom:3px; overflow:hidden;}
.pintuanDiv p{ font-size:14px; color:#666; margin-bottom:3px;}
.pintuanDiv h2{ color:#555; font-size:14px; height:30px; line-height:30px;}
.pintuanDiv h2 span{ color:#ff4d4d; padding-right:25px;}
.pintuanDiv h2 font{ color:#ff4d4d; font-size:14px; padding:0 5px;}
.pintuanDiv h3{ color:#888; font-size:13px; letter-spacing:1px;}



/*-----浮动框-----*/
.shareDiv{ background:url(../images/bg_float.png) repeat; position:absolute; top:0; left:0; z-index:998; display:none;}
span.close{ display:block; width:24px; height:24px; position:absolute; top:0; right:0; cursor:pointer; padding:6px;}
.share{ width:92%; padding:0 4%; height:197px; background:#fff; border-top:3px solid #ff4d4d; position:fixed; bottom:-200px; left:0; z-index:999;}
.share p{ font-size:14px; color:#666; letter-spacing:1px; line-height:130%; text-align:center; min-height:30px; margin-top:30px;}
.share h2{ height:20px; border-bottom:1px solid #efefef; position:relative;}
.share h2 span{ width:50px; font-size:12px; color:#666; position:absolute; top:12px; margin-left:38%; padding:0 10px; background:#fff; text-align:center;}
table.shareType{ margin-top:30px;}
table.shareType td{ text-align:center; cursor:pointer;}
table.shareType th{ font-size:12px; height:20px; color:#666; text-align:center; font-weight:normal;}





