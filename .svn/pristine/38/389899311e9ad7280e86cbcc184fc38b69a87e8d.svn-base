/*-------头部-------*/
.head{ height:47px; position:fixed; top:0; left:0; width:100%; z-index:999; background:#fff;border-bottom:1px solid #dcdcdc;}
.top-left{ position:absolute;width:38px;height:38px;top:6px;left:0%;font-size:18px;color:#FFF;}
.top-left img{width:26px; height:26px;}

.top-right{ position:absolute;width:38px;height:38px;top:6px;right:8px;font-size:18px;color:#FFF; z-index:998; }
.top-right02{ position:absolute;width:38px;height:38px;top:6px;right:52px;font-size:18px;color:#FFF; z-index:999;}
.top-right03{ position:absolute;width:38px;height:38px;top:6px;right:94px;font-size:18px;color:#FFF;}
.top-right:hover{opacity:0.7;}
.top-right_send{float:right; position:absolute; width:100px; height:40px; top:4px; right:5px; font-size:18px; color:#fff; text-align:center; line-height:40px; background:#8bcc46;}
.head h1{width:100%; font-size:18px;color:#323232;margin:0px auto; line-height:46px; text-align:center; border-bottom:1px solid #e5e5e5;position:absolute; z-index:59; border-bottom:none; color:#222;}

/*---*/
table{ border-collapse:collapse; width:100%;}
.floatLeft{ float:left;}
.floatRight{ float:right;}

/*-------我的店铺-------*/
.storte_top{width:100%;}
.bg_store{ position:relative; background:#fff;}
.wode{ position:absolute; top:20%; left:40%; width:100px;}
.wode img{ margin-left:10px;}
.wode h2{ font-size:18px; color:#fff; height:30px; line-height:30px;  text-align:center; margin-bottom:3px;}
.wode h3{ width:100px; height:26px; line-height:26px; text-align:center; background:#fff; border-radius:10px; letter-spacing:1px; color:#ff4d4d; font-size:14px;}

/*-------单品管理-------*/
.store_main{}
table.goodsType{ width:100%; height:35px; background:#fff; border-bottom:1px solid #e5e5e5; margin-bottom:10px;}
table.goodsType td a{ display:block; margin:0 10%; height:40px; line-height:40px; text-align:center; font-size:16px; color:#222; cursor:pointer; border-bottom:2px solid #fff;}
table.goodsType td a.lion{ border-bottom:2px solid #ff4d4d; color:#ff4d4d;}
.goodsList{ border-top:1px solid #e5e5e5;}
.goodsDiv{ border-bottom:1px solid #e5e5e5; padding:5px 0; background:#fff;}
.goodsHead{ padding:10px 10px 10px 120px; position:relative; height:100px;}
.goodsHead img{ position:absolute; top:10px; left:10px;}
.goodsHead h1{ height:54px; line-height:22px; font-size:16px; color:#222; margin-bottom:3px; overflow:hidden;}
.goodsHead h2{ color:#555; font-size:14px; height:30px; line-height:26px; float:left;}
.goodsHead h2 span{ font-size:15px; color:#ff4d4d; font-size:20px; padding-right:18px; float:left;}
.goodsHead h2 font{ color:#ff4d4d; font-size:14px;}
.goodsHead h3{ color:#a6a6a6; font-size:12px; padding-top:5px;}
.goodsHead h3 span{ background:#ff4d4d; width:100px; height:20px; text-align:center; color:#fff; font-size:12px; border-radius:20px; margin-right:25px; padding:0 10px;}
.kucun{background:#ff4d4d; width:100px; height:20px; text-align:center; color:#fff; font-size:12px; border-radius:20px; margin-right:25px; padding:0 10px;}


table.goodsBt{ width:100%;}
table.goodsBt td{ height:40px; line-height:40px; text-align:center; font-size:14px; color:#808080; cursor:pointer;}
table.goodsBt td span{ display:block; padding-left:25px; width:30px; margin:0 auto; position:relative;}
table.goodsBt td span img{ position:absolute; top:9px; left:0;}

/*-------特卖管理-------*/
.saleDiv{ background:#fff;}
.saleDiv img.banner{ width:100%;}
.saleDiv h1{ font-size:18px; color:#333; height:30px; line-height:30px; margin-left:15px;}
.saleDiv h1 font{ color:#ff4d4d;}
.saleDiv h2{ font-size:15px; color:#666; height:30px; line-height:20px; margin:0 15px; border-bottom:1px solid #efefef;}
.saleDiv h2 font{ font-size:18px; color:#ff4d4d; padding:3px;}

/*-------拼团管理-------*/
.pintuanDiv{ padding:15px 10px 15px 125px; position:relative; height:100px; margin-bottom:10px; background:#fff; cursor:pointer;}
.pintuanDiv img{ position:absolute; top:15px; left:15px;}
.pintuanDiv h1{ height:30px; line-height:30px; font-size:18px; color:#222; margin-bottom:3px;}
.pintuanDiv p{ font-size:14px; color:#666;}
.pintuanDiv h2{ color:#555; font-size:14px; height:30px; line-height:30px;}
.pintuanDiv h2 span{ color:#ff4d4d; padding-right:25px;}
.pintuanDiv h2 font{ color:#ff4d4d; font-size:14px; padding:0 5px;}
.pintuanDiv h3{ color:#888; font-size:13px; letter-spacing:1px;}




/*-------店铺预览-------*/
.onlineDiv{ background:#fff; padding-bottom:5px;}
.onlineDiv img{ width:100%;}

.ptDiv{ background:#fff; padding-bottom:8px;}
.ptDiv img.banner{ width:100%;}
.ptDiv h1{ font-size:18px; color:#333; height:30px; line-height:30px; margin-left:15px;}
.ptDiv h1 font{ color:#ff4d4d; padding-right:8px;}
.ptDiv h2{ font-size:14px; color:#666; height:30px; line-height:30px; margin:0 15px;}
span.ptState{ float:right; background:#ff4d4d; font-size:13px; height:24px; line-height:24px; width:60px; color:#fff; border-radius:8px; text-align:center;}


/*-------精选-------*/
.daili h2{ color:#555; font-size:14px; height:30px; line-height:26px;}
.daili h2 span{ font-size:15px; color:#ff4d4d; font-size:20px; padding-right:18px; float:left;}
.daili h2 font{ font-size:14px; color:#555; text-decoration:line-through;}
.daili h3{ color:#a6a6a6; font-size:12px;}
.daili h3 span{ background:#ff4d4d; width:100px; height:20px; text-align:center; color:#fff; font-size:12px; border-radius:20px; margin-right:35px; padding:0 10px;}

.daili{ background:#fff;}
.daili img.banner{ width:100%;}
.daili h1{ font-size:18px; color:#333; height:30px; line-height:30px; overflow:hidden; }
.daili h1 font{ color:#ff4d4d;}

.top-left{ margin-top:2px;}

.shousuoleft{ float:left; line-height:30px; padding:0px 10px 0px 20px;}
.shousuo{ color:#666; font-size:14px; float:left; background:#fff; width:100%; padding:10px 0px;}
.searchInputText{
    float:left;
    display:block;
    height:30px;
    line-height:30px;
    width:70%;
    border:1px #ececec solid;
    border-radius:3px;
    margin-right:5px;
    padding-left:10px;
    color:#999;
    font-size:12px;
    margin-left:40px;}
.fenlei{padding:0px 20px; background:#fff; margin-top:10px;}
.seek{position:absolute; top:16px; right:65px;}
.filtrate{position:absolute; top:16px; right:20px;}

.content{  padding-bottom:10px; margin:0px 10px;}

